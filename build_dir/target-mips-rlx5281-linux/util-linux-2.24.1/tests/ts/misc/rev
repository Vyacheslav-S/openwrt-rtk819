TS_TOPDIR="$(dirname $0)/../.."
TS_DESC="rev"

. $TS_TOPDIR/functions.sh
ts_init "$*"

for I in {0..512}; do printf "%s " {a..z}; done | md5sum >> $TS_OUTPUT 2>&1

for I in {0..512}; do printf "%s " {a..z}; done | \
				    $TS_CMD_REV | md5sum >> $TS_OUTPUT 2>&1

printf "abc\n123" | $TS_CMD_REV >> $TS_OUTPUT 2>&1

ts_finalize
